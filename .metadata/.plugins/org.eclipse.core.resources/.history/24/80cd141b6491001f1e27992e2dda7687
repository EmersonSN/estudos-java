		 * o valor unitário de cada peça 2. Calcule e mostre o valor a ser pago.
		 */
		Locale.setDefault(Locale.US);
		Scanner sc =  new Scanner(System.in);
		
		int id1, qtd1, id2, qtd2;
		double valor1, total1, valor2;
		
		id1 = sc.nextInt();
		qtd1 = sc.nextInt();
		
		sc.close();
	}

}
